<%= @team.name %>

<p><%= link_to "< Back", admin_teams_url %></p>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>VEC</th>
  </tr>
  
  <% @team.players.each do |player| %>
    <tr>
      <td><%= player.name %></td>
      <td><%= player.email %></td>
      <td><%= player.vec %></td>
    </tr>
  <% end %>
</table>

